    "cdk:synth": "cdk synth",
    "cdk:synth:output": "cdk synth --output=./templates",
    "cdk:synth:template": "cdk synth --no-staging > template.yaml client-file-uploader-dev",
    "bootstrap": "cdk bootstrap",
    "sam:start": "sam local start-api"
